{
  "manifest_version": 3,
  "name": "Session Switcher",
  "version": "0.1.3",
  "description": "Switch between multiple logged-in sessions on any website.",
  "permissions": [
    "cookies",
    "storage",
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}